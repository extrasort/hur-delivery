# Hur Delivery App Environment Configuration
# Copy this file to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key-here

# Mapbox Configuration
MAPBOX_ACCESS_TOKEN=your-mapbox-access-token-here
MAPBOX_SECRET_TOKEN=your-mapbox-secret-token-here

# SMS Service Configuration (handled by Supabase + Twilio integration)
# No need for direct Twilio credentials - Supabase handles this automatically

# Firebase Configuration (optional)
FIREBASE_PROJECT_ID=your-firebase-project-id

# Environment
ENVIRONMENT=development

# API Configuration
BASE_URL=https://api.hur.delivery
